                }
              },
              ".custom-bg-3:hover when (@iconsAmount > 2)": {
                "background-color": "@icon3Bg",
                ".amp-iconfont": {
                  "color": "contrast(@icon3Bg)"
                }
              },
              ".custom-bg-4:hover when (@iconsAmount > 3)": {
                "background-color": "@icon4Bg",
                ".amp-iconfont": {
                  "color": "contrast(@icon4Bg)"
                }
              },
              ".custom-bg-5:hover when (@iconsAmount > 4)": {
                "background-color": "@icon5Bg",
                ".amp-iconfont": {
                  "color": "contrast(@icon5Bg)"
                }
              },
              ".custom-bg-6:hover when (@iconsAmount > 5)": {
                "background-color": "@icon6Bg",
                ".amp-iconfont": {
                  "color": "contrast(@icon6Bg)"
                }
              }
            }
          },
