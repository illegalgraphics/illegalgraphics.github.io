          "_name": "features8",
          "_customHTML": "<section class=\"features8\" group=\"Features\" plugins=\"amp-main, font-awesome\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"0\">\n\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked>\n        <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\">\n        <input type=\"range\" inline title=\"Icons Size\" name=\"iconsSize\" min=\"1\" max=\"10\" step=\"1\" value=\"5\" condition=\"showIcons\">\n        <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#ffffff\" condition=\"showIcons\">\n        <input type=\"image\" name=\"image1\" title=\"Image 1\" value=\"@PROJECT_PATH@/assets/images/food-banner-5-1080x1080.jpg\">\n        <input type=\"image\" name=\"image2\" title=\"Image 2\" value=\"@PROJECT_PATH@/assets/images/food-banner-3-1080x1080.jpg\" condition=\"cardsAmount > 1\">\n        <input type=\"image\" name=\"image3\" title=\"Image 3\" value=\"@PROJECT_PATH@/assets/images/food-banner-1-1080x1080.jpg\" condition=\"cardsAmount > 2\">\n        <input type=\"image\" name=\"image4\" title=\"Image 4\" value=\"@PROJECT_PATH@/assets/images/food-banner-4-1080x1080.jpg\" condition=\"cardsAmount > 3\">\n        <input type=\"image\" name=\"image5\" title=\"Image 5\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" condition=\"cardsAmount > 4\">\n        <input type=\"image\" name=\"image6\" title=\"Image 6\" value=\"@PROJECT_PATH@/assets/images/background2.jpg\" condition=\"cardsAmount > 5\">\n\n        <select title=\"Cards\" name=\"cardsAmount\"